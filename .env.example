# Application
NODE_ENV=development
PORT=3000
API_PREFIX=api

# CORS
CORS_ORIGIN=*

# Database (REQUIRED)
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=user
DB_PASSWORD=password
DB_NAME=db
DB_LOGGING=false

# JWT (REQUIRED)
JWT_SECRET=your-secret-key-change-in-production-min-32-chars
JWT_EXPIRES_IN=1d

# Rate Limiting (Throttler)
THROTTLE_TTL=60
THROTTLE_LIMIT=10
